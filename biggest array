import java.util.Scanner;
public class MyClass {
    public static void main(String args[]) {
      int a[]=new int[5];
      int max;
      Scanner s=new Scanner(System.in);
      for(int i=0;i<a.length;i++)
      {
          a[i]=s.nextInt();
      }
      max=a[0];
        for(int i=0;i<a.length;i++)
        {
            if(max>a[i])
            {
                max=a[i];
            }
            
        }
        

      System.out.println(max);
    }
}
